{
    "extends": ["eslint:recommended", "prettier"],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
        "prettier/prettier": "error"
      },
      "plugins": [
        "prettier",
        "@babel/plugin-syntax-dynamic-import"
      ],
      "parserOptions": {
        "sourceType": "module"
    }
  }
